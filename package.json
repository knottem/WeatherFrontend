{
  "name": "weather-frontend",
  "version": "0.7.3",
  "description": "Weather frontend application",
  "scripts": {
    "prebuild": "node ./scripts/set-version.js",
    "prebuild:android": "node ./scripts/set-version.js",
    "prestart": "node ./scripts/set-dev-version.js",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:android": "ng build --configuration production && npx cap sync",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.5",
    "@angular/cdk": "^17.3.6",
    "@angular/common": "^17.3.5",
    "@angular/compiler": "^17.3.5",
    "@angular/core": "^17.3.5",
    "@angular/forms": "^17.3.5",
    "@angular/material": "^17.3.6",
    "@angular/platform-browser": "^17.3.5",
    "@angular/platform-browser-dynamic": "^17.3.5",
    "@angular/router": "^17.3.5",
    "@capacitor-community/screen-brightness": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@ionic/angular": "^8.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-svg-icon": "^17.0.0",
    "luxon": "^3.4.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "vite": "^5.2.10",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.5",
    "@capacitor/cli": "6.1.2",
    "@types/jasmine": "~4.3.0",
    "@types/luxon": "^3.3.7",
    "autoprefixer": "^10.4.17",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "5.2"
  }
